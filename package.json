{
  "name": "reactstrap-sass",
  "version": "0.0.1",
  "description": "Use reactstrap with per component SASS instead of the entire Bootstrap CSS.",
  "keywords": [
    "react",
    "Bootstrap",
    "reactstrap"
  ],
  "author": "jmlivingston@gmail.com",
  "license": "ISC",
  "repository": "https://github.com/jmlivingston/reactstrap-sass",
  "scripts": {
    "build": "rollup -c",
    "start": "cross-env STORYBOOK_DEBUG=true start-storybook -p 4000 --quiet --ci",
    "start:info": "start-storybook -p 4000 --quiet --ci",
    "test:dev": "jest --watch",
    "test": "jest",
    "prepublish-documentation": "build-storybook -o storybook-static",
    "publish-documentation": "storybook-to-ghpages",
    "start-documentation": "npx http-server storybook-static",
    "lint": "node node_modules/eslint/bin/eslint.js --max-warnings=0 src",
    "precommit": "lint-staged",
    "generate": "node --experimental-modules node_modules/.bin/plop --plopfile scripts/templates/index.js",
    "react-strap-build-schema": "node --experimental-modules scripts/reactstrap/build-schema.mjs",
    "react-strap-build-components": "node --experimental-modules scripts/reactstrap/build-components.mjs",
    "react-strap-build-examples": "nodemon --experimental-modules scripts/reactstrap/build-examples.mjs --watch scripts/reactstrap/build-examples.mjs"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-a11y": "5.1.3",
    "@storybook/addon-actions": "5.1.3",
    "@storybook/addon-info": "4.1.11",
    "@storybook/addon-links": "5.1.3",
    "@storybook/addon-storysource": "5.1.3",
    "@storybook/addon-viewport": "5.1.3",
    "@storybook/addons": "5.1.3",
    "@storybook/react": "5.1.3",
    "@storybook/storybook-deployer": "2.8.1",
    "@testing-library/react": "8.0.1",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.6",
    "bootstrap": "4.3.1",
    "bootswatch": "4.2.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-dom": "3.4.0",
    "lint-staged": "8.1.7",
    "node-sass": "4.12.0",
    "plop": "2.3.0",
    "prettier": "1.17.1",
    "prop-types": "15.7.2",
    "react-hooks-testing-library": "0.5.1",
    "react-test-renderer": "16.8.6",
    "reactstrap": "8.0.0",
    "rimraf": "2.6.3",
    "rollup": "1.1.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-filesize": "6.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-terser": "5.0.0",
    "sass": "1.20.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1"
  },
  "peerDependencies": {
    "react": "16.*",
    "react-dom": "16.*"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/style.mock.js"
    },
    "transform": {
      ".+\\.js$": "babel-jest"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "no-console": 1,
      "react/default-props-match-prop-types": 1,
      "react/no-unused-prop-types": 1,
      "react/prop-types": 1,
      "react/sort-prop-types": 1,
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "react": {
        "version": "latest"
      }
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,json,css}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.js": [
        "npm run lint",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
